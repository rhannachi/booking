{
  "extends": "@rhannachi/eslint-config-prettier/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src",
    "rootDir": "src",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "incremental": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "src/**/*",
    "custom.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
